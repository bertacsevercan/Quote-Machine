{
  "expo": {
    "name": "Quote Machine",
    "slug": "quoteMachine",
    "scheme": "quotemachine",
    "description": "Random quotes from your favourite tv shows, people and authors.",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "privacy": "public",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor":  "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pizzabyte.quotemachine",
      "buildNumber": "1.0.2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package":  "com.pizzabyte.quotemachine",
      "versionCode": 1.2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
